@model ReservationSystem.Models.GuestModel
@{
    ViewBag.Title = "displayGuest";
}

<h2>displayGuest</h2>
<form action="Index">

    <table style="border:solid">
        <thead>
            <tr>
                <td> GuestName </td>
                <td> ReservationId </td>
            </tr>
        </thead>

        @foreach (var Guest in ViewBag.guest)
        {
            <tr>
                <td>@Guest.GuestName</td>
                <td>@Guest.ReservationId</td>

            </tr>
        }
    </table>
    <br />
    <input type="submit" />
</form>

